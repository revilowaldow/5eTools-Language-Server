{
	"_meta": {
		"prefix": "meta",
		"body": [
			"\"_meta\": {",
			"\t\"sources\": [",
			"\t\t{",
			"\t\t\t\"json\": \"$1\",",
			"\t\t\t\"abbreviation\": \"$2\",",
			"\t\t\t\"full\": \"$3\",",
			"\t\t\t\"authors\": [",
			"\t\t\t\t\"$4\"",
			"\t\t\t],",
			"\t\t\t\"convertedBy\": [",
			"\t\t\t\t\"$5\"",
			"\t\t\t],",
			"\t\t\t\"version\": \"$6\",",
			"\t\t\t\"color\": \"${7:ffffff}\",",
			"\t\t\t\"colorNight\": \"${8:ffffff}\",",
			"\t\t\t\"url\": \"${9:https://github.com/TheGiddyLimit/homebrew}\",",
			"\t\t\t\"dateReleased\": \"${10:yyyy-mm-dd}\"",
			"\t\t}",
			"\t],",
			"\t\"edition\": \"${11|classic,one|}\",",
			"\t\"status\": \"${12|wip,deprecated,ready,invalid|}\",",
			"\t\"dateAdded\": $CURRENT_SECONDS_UNIX,",
			"\t\"dateLastModified\": $CURRENT_SECONDS_UNIX",
			"}"
		],
		"description": "Full basic _meta"
	},
	"unixDate": {
		"prefix": "unixDate",
		"body": [
			"$CURRENT_SECONDS_UNIX"
		],
		"description": "The number of seconds since the Unix epoch"
	},
	"$schema": {
		"prefix": "$schema",
		"body": [
			"\"\\$schema\": \"https://raw.githack.com/TheGiddyLimit/5etools-utils/master/schema/brew${1:-fast}/homebrew.json\""
		],
		"description": "The JSON Schema for 5eTools Homebrew"
	}
}
